<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".inspection.InspectionRoughDeviceListActivity">

        <com.example.new_androidclient.customize_view.TitleLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:name="@string/inspection_rough_device_list" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/inspection_rough_device_list_recycler"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
        <com.example.new_androidclient.customize_view.NoDataLayout
            android:id="@+id/inspection_rough_device_list_nodata"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"/>

    </LinearLayout>
</layout>