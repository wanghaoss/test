<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleLayout">
        <attr name="name" format="string"/>
        <attr name="showMorePic" format="boolean"/>
        <attr name="showBlueTooth" format="boolean"/>
        <attr name="showHazardPic" format="boolean"/>
        <attr name="showHazardUploadFile" format="boolean"/>
        <attr name="showLogout" format="boolean"/>
        <attr name="showEditionAddNotice" format="boolean"/>
        <attr name="work_switch" format="boolean"/>
        <attr name="moduleName">
            <enum name="inspection" value="1"/>
            <enum name="wait_inspection" value="2"/>
            <enum name="main" value="3"/>
            <enum name="hazard_pic" value="4"/>
            <enum name="hazard_upload" value="5"/>
        </attr>
        <attr name="search" format="boolean"/>
    </declare-styleable>
</resources>